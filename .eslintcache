r\n            ))}\r\n        </ul>\r\n    )\r\n\r\n    \r\n\r\n    const cartModalContent = (<React.Fragment>\r\n        {cartItems}\r\n        <div className={classes.total}>\r\n            <p>Total Amount</p>\r\n            <p>{totalAmount}</p>\r\n        </div>\r\n        {!isVisible ?\r\n        <div className={classes.actions}>\r\n            <button className={classes['btn--alt']} onClick={props.notVisible}>Close</button>\r\n            {hasItems && <button className={classes.btn} onClick={orderClickHandler}>Order</button>}\r\n        </div> : <CheckoutForm onSubmit={submitFormHandler} close={cancelFormHandler}/>}\r\n        </React.Fragment>)\r\n         \r\n         \r\n    return(\r\n        \r\n        <Modal onClick={props.notVisible}>\r\n        {formSubmitted ? <div><h1>thank you for your order!</h1><button onClick={props.notVisible}>close</button></div> : cartModalContent}\r\n        </Modal>\r\n        \r\n    )\r\n}\r\nexport default Cart","C:\\food-order-app\\src\\components\\UI\\Input.js",[],"C:\\food-order-app\\src\\components\\UI\\Modal.js",[],"C:\\food-order-app\\src\\store\\CartProvider.js",[],"C:\\food-order-app\\src\\store\\cart-context.js",[],"C:\\food-order-app\\src\\components\\Cart\\CartItem.js",[],"C:\\food-order-app\\src\\form\\CheckoutForm.js",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":1,"column":15,"nodeType":"87","messageId":"88","endLine":1,"endColumn":23},{"ruleId":"85","severity":1,"message":"89","line":4,"column":36,"nodeType":"87","messageId":"88","endLine":4,"endColumn":49},"no-native-reassign",["90"],"no-negated-in-lhs",["91"],"no-unused-vars","'Fragment' is defined but never used.","Identifier","unusedVar","'ReactFragment' is defined but never used.","no-global-assign","no-unsafe-negation"]